    <key n="125">
      <name>}</name>
      <desc>Engine Autostart</desc>
      <binding>
        <command>nasal</command>
        <script>engines.autostart();</script>
      </binding>
    </key>
    
    <key n="108">
      <name>l</name>
      <desc>Landing light</desc>
      <binding>
        <command>property-toggle</command>
        <property>/controls/lighting/landing-lights</property>
      </binding>
    </key>

    <key n="100">
      <name>d</name>
      <desc>Deploy braking parachute</desc>
      <binding>
        <command>property-assign</command>
        <property>controls/flight/drag-chute</property>
        <value type="int">1</value>
      </binding>
    </key>

    <key n="106">
      <name>j</name>
      <desc>jettison braking parachute</desc>
      <binding>
        <command>property-assign</command>
        <property>controls/flight/drag-chute</property>
        <value type="int">0</value>
      </binding>
      <binding>
        <command>property-assign</command>
        <property>controls/flight/chute_jettisoned</property>
        <value type="int">1</value>
      </binding>
    </key>

    
  </keyboard>
  
